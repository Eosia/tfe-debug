RewriteEngine on
RewriteRule .* https://%{HTTP_HOST}%{REQUEST_URI} [R=301,L]

RewriteEngine On
RewriteCond %{SERVER_PORT} 80
RewriteRule ^(.*)$ https://internship-me.be/$1 [R,L]


RewriteEngine on
RewriteCond %{HTTP_HOST} ^internship-me.be$ [NC,OR]
RewriteCond %{HTTP_HOST} ^www.internship-me.be$
RewriteCond %{REQUEST_URI} !public/
RewriteRule (.*) /public/$1 [L]
